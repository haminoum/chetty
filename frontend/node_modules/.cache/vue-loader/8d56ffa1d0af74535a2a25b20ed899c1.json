sages\" :key=\"msg\">\n          <ChatBubble :message=\"msg\" />\n        </div>\n        <div class=\"flex\">\n          <input\n            class=\"p-2 w-full rounded-lg\"\n            type=\"text\"\n            name=\"message\"\n            id=\"message\"\n            placeholder=\"message\"\n            v-model=\"form.message\"\n          />\n\n          <button\n            class=\"\n              button-primary\n              hover:bg-green-600\n              shadow-md\n              text-lg\n              font-semibold\n            \"\n            @click=\"sendMessage()\"\n          >\n            Send\n          </button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport ChatBubble from \"./ChatBubble.vue\";\nimport Users from \"./Users.vue\";\nimport { reactive } from \"vue\";\n\nexport default {\n  props: {\n    messages: { required: true, type: Object },\n    users: { required: true, type: Object },\n    connected: { required: true, type: Boolean },\n    ws: { required: true, type: Object },\n  },\n  components: {\n    ChatBubble,\n    Users,\n  },\n  mounted() {},\n  data() {\n    var form = reactive({\n      message: null,\n    });\n    const messageDestination = \"/app/chat/\";\n    return {\n      form,\n      messageDestination,\n    };\n  },\n  methods: {\n    sendMessage() {\n      let user = \"hero\";\n      let header = {};\n\n      let message = {\n        type: \"CHAT\",\n        content: this.form.message ? this.form.message : \"Sample Msg\",\n        sender: user,\n      };\n\n      if (this.connected) {\n        this.ws.send(\n          this.messageDestination + user,\n          JSON.stringify(message),\n          header\n        );\n\n        console.log(\"MESSAGES\", this.messages);\n      } else {\n        alert(\"Connection lost...\");\n      }\n    },\n  },\n};\n</script>\n\n<style>\n</style>"]}]}